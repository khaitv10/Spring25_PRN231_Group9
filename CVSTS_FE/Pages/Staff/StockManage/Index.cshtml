@page
@model CVSTS_FE.Pages.Staff.StockManage.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VaccineStock[0].Vaccine.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VaccineStock[0].Vaccine.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VaccineStock[0].Quantity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VaccineStock[0].ExpiryDate)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.VaccineStock) {
        <tr>
            <td>
                 @Html.DisplayFor(modelItem => item.Vaccine.Name)
            </td>
            <td>
                 @Html.DisplayFor(modelItem => item.Vaccine.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExpiryDate)
            </td>

            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
